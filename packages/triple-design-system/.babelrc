{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "ie": "11",
          "node": "6"
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    ["@babel/plugin-proposal-class-properties"]
  ]
}
